Gamer:Button("Help", function()
local msg = Instance.new("Message", player.PlayerGui)
msg.Text = "Script made by Wide. If you press Fly you will enable the fly button (P). Inf Jump will enable the inf jump button (L). The Tools give you some asome Tools. T to close this Mesage"

mouse.KeyDown:Connect(function(key)
    if key == "t" then
        msg:Destroy()
    end
end)
end)

